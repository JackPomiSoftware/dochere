# Основные настройки приложения
NODE_ENV=production
VITE_APP_TITLE=DocHere
VITE_APP_VERSION=1.0.0

# API настройки
VITE_API_URL=https://your-api-domain.com/api
VITE_API_TIMEOUT=10000

# Настройки базы данных (если используется)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=dochere
# DB_USER=your_db_user
# DB_PASSWORD=your_db_password

# Настройки Redis (если используется)
# REDIS_HOST=localhost
# REDIS_PORT=6379
# REDIS_PASSWORD=your_redis_password

# Настройки почты (если используется)
# MAIL_HOST=smtp.gmail.com
# MAIL_PORT=587
# MAIL_USER=your_email@gmail.com
# MAIL_PASSWORD=your_email_password

# Настройки JWT (если используется)
# JWT_SECRET=your_jwt_secret_key
# JWT_EXPIRES_IN=7d

# Настройки файлового хранилища
# UPLOAD_PATH=./uploads
# MAX_FILE_SIZE=10485760

# Настройки логирования
# LOG_LEVEL=info
# LOG_FILE=./logs/app.log

# Настройки для разработки
# VITE_DEV_SERVER_PORT=3000
# VITE_DEV_SERVER_HOST=0.0.0.0

# Настройки для продакшена
# VITE_BUILD_OUTDIR=dist
# VITE_BUILD_ASSETSDIR=assets

# Настройки безопасности
# CORS_ORIGIN=https://your-domain.com
# RATE_LIMIT_WINDOW=15
# RATE_LIMIT_MAX=100

# Настройки мониторинга (если используется)
# SENTRY_DSN=your_sentry_dsn
# ANALYTICS_ID=your_analytics_id 